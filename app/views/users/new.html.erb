<h1> Register </h1>

<%= form_for :user, url: '/users' do |f| %>
  <p>First Name 
    <br> 
    <%= f.text_field :first_name %>
  </p>
  <p>Last Name 
    <br>
    <%= f.text_field :last_name %>
  </p>
  <p>Email 
    <br>
    <%= f.text_field :email %>
  </p>
  <p>Password 
    <br>
    <%= f.password_field :password %>
  </p>
  <p>Confirm Password 
    <br>
    <%= f.password_field :password_confirmation %>
  </p>
  <br>
    <%= f.submit "Register" %>
  </br>
<% end %>